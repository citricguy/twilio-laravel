{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "none"
        },
        "not_operator_with_successor_space": true,
        "simplified_null_return": true,
        "phpdoc_separation": false,
        "phpdoc_align": {
            "align": "left"
        }
    }
}
